<launch>

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <param name = "base_global_planner" value = "navfn/NavfnROS"/>
        <!-- <param name = "base_local_planner" value = "dwa_local_planner/DWAPlannerROS"/> -->

        <!-- <param name="recovery_behavior_enabled" value="false" /> -->
        

        <rosparam file="$(find my_planner)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find my_planner)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find my_planner)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find my_planner)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find my_planner)/param/base_local_planner_params.yaml" command="load" />
    </node>

</launch>
